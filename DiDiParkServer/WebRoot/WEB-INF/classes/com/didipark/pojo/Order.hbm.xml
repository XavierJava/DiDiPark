<?xml version="1.0" encoding="GBK"?>
<!-- 指定Hibernate映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.didipark.pojo">
	<!-- 映射User持久化类 -->
	<class name="Order" table="tb_OrderInfo">
		<!-- 映射标识属性 -->
		<id name="id" column="order_id">

		</id>

		<property name="carport_id" column="order_carport_id" />
		<property name="user_id" column="order_user_id" />
		<property name="time" column="order_time" />
		<property name="type" column="order_type" />
		<property name="money" column="order_money" />
		<property name="commentId">
			<column name="order_comment_id" default="0" />
		</property>


		<!-- 映射普通属性: -->

	</class>

</hibernate-mapping>
